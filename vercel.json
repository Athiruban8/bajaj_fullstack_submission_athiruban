{
  "version": 2,
  "functions": {
    "api/bfhl.js": {
      "maxDuration": 10
    },
    "api/health.js": {
      "maxDuration": 5
    },
    "api/index.js": {
      "maxDuration": 5
    }
  },
  "routes": [
    {
      "src": "/bfhl",
      "dest": "/api/bfhl.js"
    },
    {
      "src": "/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
